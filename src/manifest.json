{
    "name": "GitHub Notifications Bot",
    "version": "1.0",
    "options_page": "options.html",
    "description": "Build an Extension!",
    "permissions": ["storage", "declarativeContent"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/ncsu_logo.png",
        "32": "images/ncsu_logo.png",
        "48": "images/ncsu_logo.png",
        "128": "images/ncsu_logo.png"
      }
    },
     "icons": {
      "16": "images/ncsu_logo.png",
      "32": "images/ncsu_logo.png",
      "48": "images/ncsu_logo.png",
      "128": "images/ncsu_logo.png"
    },
    "manifest_version":2
}
